CREATE TABLE NEXI_CHAT (
     SEQ		VARCHAR2(1000)	NOT NULL 
    ,SUBJECT	VARCHAR2(100)	NOT NULL
    ,USERID		VARCHAR2(100)	
    ,IDATE       DATE
    ,DELETEYN	VARCHAR2(1)	NOT NULL 
);

COMMENT ON COLUMN NEXI_CHAT.SEQ IS '시퀀스';
COMMENT ON COLUMN NEXI_CHAT.SUBJECT IS '채팅방제목';
COMMENT ON COLUMN NEXI_CHAT.USERID IS '채팅방만든유저아이디';
COMMENT ON COLUMN NEXI_CHAT.IDATE IS '채팅방만든날짜';
COMMENT ON COLUMN NEXI_CHAT.DELETEYN IS '채팅방존재유무';